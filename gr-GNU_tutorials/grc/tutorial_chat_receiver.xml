<?xml version="1.0"?>
<block>
  <name>Chat Receiver</name>
  <key>tutorial_chat_receiver</key>
  <category>Tutorial</category>
  <import>import tutorial</import>
  <make>tutorial.chat_receiver($metadata)</make>

  <param>
    <name>Show Metadata</name>
    <key>metadata</key>
    <type>enum</type>
    <option>
      <name>Yes</name>
      <key>"yes"</key>
      <opt>show:True</opt>
    </option>
    <option>
      <name>No</name>
      <key>"no"</key>
      <opt>show:False</opt>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>