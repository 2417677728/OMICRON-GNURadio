<?xml version="1.0"?>
<block>
  <name>My QPSK Demodulator (py)</name>
  <key>GNU_tutorials_qpsk_demod_py_cb</key>
  <category>[GNU_tutorials]</category>
  <import>import GNU_tutorials</import>
  <make>GNU_tutorials.qpsk_demod_py_cb($gray_code)</make>

  <param>
    <name>Gray Code</name>
    <key>gray_code</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
