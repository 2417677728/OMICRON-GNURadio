<?xml version="1.0"?>
<block>
  <name>My QPSK Demodulator (tagged)</name>
  <key>GNU_tutorials_qpsk_tagged_demod_cb_py</key>
  <category>[GNU_tutorials]</category>
  <import>import GNU_tutorials</import>
  <make>GNU_tutorials.qpsk_tagged_demod_cb_py($gray_code)</make>

  <param>
    <name>Gray Code</name>
    <key>gray_code</key>
    <value>True</value>
    <type>int</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
