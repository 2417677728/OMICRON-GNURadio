<?xml version="1.0"?>
<block>
  <name>Parse MAC</name>
  <key>frequencyAdaptiveOFDM_parse_mac</key>
  <category>[Frequency Adaptive OFDM]</category>
  <import>import frequencyAdaptiveOFDM</import>
  <make>frequencyAdaptiveOFDM.parse_mac($m_and_p, $src_mac, $debug, $rx_packets_f)</make>

  <param>
    <name>MAC &amp; Parse</name>
    <key>m_and_p</key>
    <type>mac_and_parse</type>
  </param>

  <param>
    <name>SRC MAC</name>
    <key>src_mac</key>
    <value>[0x23, 0x23, 0x23, 0x23, 0x23, 0x23]</value>
    <type>int_vector</type>
  </param>

  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>

  <param>
    <name># Rx Packets File</name>
    <key>rx_packets_f</key>
    <value>/tmp/rx_packets.csv</value>
    <type>string</type>
  </param>

  <check>len($src_mac) == 6</check>
  <check>all([x >= 0 and 255 >= x for x in $src_mac])</check>


  <sink>
    <name>phy in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>app out</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <source>
    <name>fer</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <source>
    <name>frame data</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>

